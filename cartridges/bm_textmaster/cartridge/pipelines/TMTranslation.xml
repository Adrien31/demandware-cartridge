<?xml version="1.0" encoding="UTF-8" ?>
<?demandware-pipeline version="2.0"?>

<pipeline type="TextMasterProject">
  <branch basename="_ANONYMOUS_BRANCH_1">
    <segment>
      <node>
        <text-node>
          <description>Form to show filter options for translation</description>
        </text-node>
        <node-display x="1" y="0"/>
      </node>
    </segment>
  </branch>
  <branch basename="_ANONYMOUS_BRANCH_2">
    <segment>
      <node>
        <text-node>
          <description>Show translation follow up status</description>
        </text-node>
        <node-display x="3" y="0"/>
      </node>
    </segment>
  </branch>
  <branch basename="New">
    <segment>
      <node>
        <start-node name="New" secure="false"/>
        <node-display x="1" y="1"/>
      </node>
      <simple-transition/>
      <node>
        <call-node start-name-ref="TMTranslation-LoginCheck"/>
        <node-display x="0" y="1"/>
        <branch basename="b2" source-connector="Registered">
          <transition target-connector="in"/>
          <segment>
            <node>
              <interaction-node transaction-required="false">
                <template buffered="true" dynamic="false" name="translation/filtertranslationitems"/>
              </interaction-node>
              <node-display x="0" y="1"/>
            </node>
          </segment>
        </branch>
      </node>
    </segment>
  </branch>
  <branch basename="FollowUp">
    <segment>
      <node>
        <start-node name="FollowUp" secure="false"/>
        <node-display x="3" y="1"/>
      </node>
      <simple-transition/>
      <node>
        <call-node start-name-ref="TMTranslation-LoginCheck"/>
        <node-display x="0" y="1"/>
        <branch basename="b2" source-connector="Registered">
          <transition target-connector="in">
            <transition-display>
              <bend-point relative-to="source" x="0" y="1"/>
            </transition-display>
          </transition>
          <segment>
            <node>
              <interaction-node transaction-required="false">
                <template buffered="true" dynamic="false" name="translation/followupontranslation"/>
              </interaction-node>
              <node-display x="0" y="1"/>
            </node>
          </segment>
        </branch>
      </node>
    </segment>
  </branch>
  <branch basename="_ANONYMOUS_BRANCH_5">
    <segment>
      <node>
        <text-node>
          <description>Regitration link to TextMaster</description>
        </text-node>
        <node-display x="5" y="0"/>
      </node>
    </segment>
  </branch>
  <branch basename="Register">
    <segment>
      <node>
        <start-node name="Register" secure="false"/>
        <node-display x="5" y="1"/>
      </node>
      <simple-transition/>
      <node>
        <pipelet-node pipelet-name="Script" pipelet-set-identifier="bc_api">
          <config-property key="Transactional" value="false"/>
          <config-property key="OnError" value="PIPELET_ERROR"/>
          <config-property key="ScriptFile" value="translation/GetAPIConfigurations.js"/>
          <key-binding alias="null" key="ScriptLog"/>
          <key-binding alias="APIKey" key="APIKey"/>
          <key-binding alias="APISecret" key="APISecret"/>
          <key-binding alias="APICategories" key="APICategories"/>
          <key-binding alias="APICategory" key="APICategory"/>
          <key-binding alias="APICatalogID" key="APICatalogID"/>
          <key-binding alias="APIEnv" key="APIEnv"/>
          <key-binding alias="TMPageSize" key="TMPageSize"/>
          <key-binding alias="APICache" key="APICache"/>
          <key-binding alias="TMSFpassword" key="TMSFpassword"/>
          <key-binding alias="tmApiBaseUrlDemo" key="tmApiBaseUrlDemo"/>
          <key-binding alias="tmApiBaseUrlLive" key="tmApiBaseUrlLive"/>
          <key-binding alias="tmBackofficeBaseUrlLive" key="tmBackofficeBaseUrlDemo"/>
          <key-binding alias="tmBackofficeBaseUrlDemo" key="tmBackofficeBaseUrlLive"/>
        </pipelet-node>
        <node-display x="0" y="1"/>
        <branch basename="b2" source-connector="error">
          <transition target-connector="in">
            <transition-display>
              <bend-point relative-to="source" x="1" y="0"/>
            </transition-display>
          </transition>
          <segment>
            <node>
              <end-node name="Error"/>
              <node-display orientation="horizontal" x="1" y="0"/>
            </node>
          </segment>
        </branch>
      </node>
      <simple-transition/>
      <node>
        <interaction-node transaction-required="false">
          <template buffered="true" dynamic="false" name="translation/entertextmaster"/>
        </interaction-node>
        <node-display x="0" y="1"/>
      </node>
    </segment>
  </branch>
  <branch basename="_ANONYMOUS_BRANCH_7">
    <segment>
      <node>
        <text-node>
          <description>Subpipeline 'New' posts data to this subpipeline</description>
        </text-node>
        <node-display x="1" y="7"/>
      </node>
    </segment>
  </branch>
  <branch basename="_ANONYMOUS_BRANCH_8">
    <segment>
      <node>
        <text-node>
          <description>Notification page on project creation</description>
        </text-node>
        <node-display x="3" y="7"/>
      </node>
    </segment>
  </branch>
  <branch basename="_ANONYMOUS_BRANCH_9">
    <segment>
      <node>
        <text-node>
          <description>Default attributes settings</description>
        </text-node>
        <node-display x="5" y="7"/>
      </node>
    </segment>
  </branch>
  <branch basename="PlaceOrder">
    <segment>
      <node>
        <start-node name="PlaceOrder" secure="false"/>
        <node-display x="1" y="8"/>
      </node>
      <simple-transition/>
      <node>
        <pipelet-node pipelet-name="Script" pipelet-set-identifier="bc_api">
          <config-property key="Transactional" value="false"/>
          <config-property key="OnError" value="PIPELET_ERROR"/>
          <config-property key="ScriptFile" value="translation/SetupTranslationParameters.js"/>
          <key-binding alias="null" key="ScriptLog"/>
          <key-binding alias="CurrentHttpParameterMap.get(&quot;locale-from&quot;).stringValue" key="LocaleFrom"/>
          <key-binding alias="TransParams" key="TransParams"/>
          <key-binding alias="CurrentHttpParameterMap.get(&quot;attribute[]&quot;).values.toArray()" key="Attributes"/>
          <key-binding alias="CurrentHttpParameterMap.get(&quot;items&quot;).stringValue" key="Items"/>
          <key-binding alias="CurrentHttpParameterMap.get(&quot;locale-to[]&quot;).values.toArray()" key="LocaleTo"/>
          <key-binding alias="CurrentHttpParameterMap.get(&quot;item-type&quot;).stringValue" key="ItemType"/>
          <key-binding alias="CurrentHttpParameterMap.get(&quot;catalog&quot;).stringValue" key="CatalogID"/>
        </pipelet-node>
        <node-display x="0" y="1"/>
        <branch basename="b2" source-connector="error">
          <transition target-connector="in">
            <transition-display>
              <bend-point relative-to="source" x="1" y="0"/>
            </transition-display>
          </transition>
          <segment>
            <node>
              <end-node name="Error"/>
              <node-display orientation="horizontal" x="1" y="0"/>
            </node>
          </segment>
        </branch>
      </node>
      <simple-transition/>
      <node>
        <interaction-node transaction-required="false">
          <template buffered="true" dynamic="false" name="translation/placeorder"/>
        </interaction-node>
        <node-display x="0" y="1"/>
      </node>
    </segment>
  </branch>
  <branch basename="Notification">
    <segment>
      <node>
        <start-node name="Notification" secure="false"/>
        <node-display x="3" y="8"/>
      </node>
      <simple-transition/>
      <node>
        <pipelet-node pipelet-name="Assign" pipelet-set-identifier="bc_api">
          <config-property key="Transactional" value="false"/>
          <key-binding alias="CurrentHttpParameterMap.get(&quot;autoLaunchCount&quot;).intValue" key="From_0"/>
          <key-binding alias="autoLaunchCount" key="To_0"/>
          <key-binding alias="CurrentHttpParameterMap.get(&quot;noAutoLaunchCount&quot;).intValue" key="From_1"/>
          <key-binding alias="noAutoLaunchCount" key="To_1"/>
          <key-binding alias="CurrentHttpParameterMap.get(&quot;projectID&quot;).stringValue" key="From_2"/>
          <key-binding alias="projectID" key="To_2"/>
          <key-binding alias="null" key="From_3"/>
          <key-binding alias="null" key="To_3"/>
          <key-binding alias="null" key="From_4"/>
          <key-binding alias="null" key="To_4"/>
          <key-binding alias="null" key="From_5"/>
          <key-binding alias="null" key="To_5"/>
          <key-binding alias="null" key="From_6"/>
          <key-binding alias="null" key="To_6"/>
          <key-binding alias="null" key="From_7"/>
          <key-binding alias="null" key="To_7"/>
          <key-binding alias="null" key="From_8"/>
          <key-binding alias="null" key="To_8"/>
          <key-binding alias="null" key="From_9"/>
          <key-binding alias="null" key="To_9"/>
        </pipelet-node>
        <node-display x="0" y="1"/>
      </node>
      <simple-transition/>
      <node>
        <interaction-node transaction-required="false">
          <template buffered="true" dynamic="false" name="translation/notification"/>
        </interaction-node>
        <node-display x="0" y="1"/>
      </node>
    </segment>
  </branch>
  <branch basename="DefaultAttributes">
    <segment>
      <node>
        <start-node name="DefaultAttributes" secure="false"/>
        <node-display x="5" y="8"/>
      </node>
      <simple-transition>
        <transition-display>
          <bend-point relative-to="source" x="0" y="1"/>
        </transition-display>
      </simple-transition>
      <node>
        <interaction-node transaction-required="false">
          <template buffered="true" dynamic="false" name="translation/defaultattributessettings"/>
        </interaction-node>
        <node-display x="0" y="1"/>
      </node>
    </segment>
  </branch>
  <branch basename="_ANONYMOUS_BRANCH_13">
    <segment>
      <node>
        <text-node>
          <description>Check API key and API secret entered in Site Preference</description>
        </text-node>
        <node-display x="1" y="12"/>
      </node>
    </segment>
  </branch>
  <branch basename="LoginCheck">
    <segment>
      <node>
        <start-node call-mode="public" name="LoginCheck" secure="false"/>
        <node-display x="1" y="13"/>
      </node>
      <simple-transition>
        <transition-display>
          <bend-point relative-to="source" x="0" y="1"/>
        </transition-display>
      </simple-transition>
      <node>
        <pipelet-node pipelet-name="Assign" pipelet-set-identifier="bc_api">
          <config-property key="Transactional" value="false"/>
          <key-binding alias="dw.system.Site.current.getCustomPreferenceValue(&quot;TMApiKey&quot;) || &quot;&quot;" key="From_0"/>
          <key-binding alias="APIKey" key="To_0"/>
          <key-binding alias="dw.system.Site.current.getCustomPreferenceValue(&quot;TMApiSecret&quot;) || &quot;&quot;" key="From_1"/>
          <key-binding alias="APISecret" key="To_1"/>
          <key-binding alias="null" key="From_2"/>
          <key-binding alias="null" key="To_2"/>
          <key-binding alias="null" key="From_3"/>
          <key-binding alias="null" key="To_3"/>
          <key-binding alias="null" key="From_4"/>
          <key-binding alias="null" key="To_4"/>
          <key-binding alias="null" key="From_5"/>
          <key-binding alias="null" key="To_5"/>
          <key-binding alias="null" key="From_6"/>
          <key-binding alias="null" key="To_6"/>
          <key-binding alias="null" key="From_7"/>
          <key-binding alias="null" key="To_7"/>
          <key-binding alias="null" key="From_8"/>
          <key-binding alias="null" key="To_8"/>
          <key-binding alias="null" key="From_9"/>
          <key-binding alias="null" key="To_9"/>
        </pipelet-node>
        <node-display x="0" y="1"/>
      </node>
      <simple-transition>
        <transition-display>
          <bend-point relative-to="source" x="0" y="1"/>
        </transition-display>
      </simple-transition>
      <node>
        <decision-node condition-key="APIKey != &quot;&quot; &amp;&amp; APISecret != &quot;&quot;" condition-operator="expr"/>
        <node-display x="0" y="1"/>
        <branch basename="b2" source-connector="yes">
          <transition target-connector="in">
            <transition-display>
              <bend-point relative-to="source" x="0" y="1"/>
            </transition-display>
          </transition>
          <segment>
            <node>
              <end-node name="Registered"/>
              <node-display x="0" y="1"/>
            </node>
          </segment>
        </branch>
      </node>
      <simple-transition>
        <transition-display>
          <bend-point relative-to="source" x="1" y="0"/>
        </transition-display>
      </simple-transition>
      <node>
        <jump-node start-name-ref="TMTranslation-Register"/>
        <node-display orientation="horizontal" x="1" y="0"/>
      </node>
    </segment>
  </branch>
</pipeline>
